public def factorial(n) { // Dynamic typing
  if (n <= 1) {
    1 // No return keyword, no ";" at the end of the last line
  } else { // else is mandatory
    n * factorial(n - 1) // infix operators, recursion
  }
}

self.announce(factorial(4));
